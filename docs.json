{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Qualia Docs",
  "colors": {
    "primary": "#66349a",
    "light": "#8b5fc4",
    "dark": "#4a2070"
  },
  "appearance": {
    "default": "light"
  },
  "background": {
    "decoration": "gradient",
    "color": {
      "light": "#fffaff",
      "dark": "#1a1025"
    }
  },
  "favicon": "/favicon.png",
  "fonts": {
    "heading": {
      "family": "Funkis",
      "weight": 150,
      "source": "/fonts/FunkisABCVariable.woff2",
      "format": "woff2"
    },
    "body": {
      "family": "IBM Plex Sans"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": ["index", "getting-started"]
          },
          {
            "group": "Using Qualia",
            "pages": ["chat-and-ai", "notebooks", "agents", "settings"]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Changelog",
          "href": "https://quadrillion.io/changelog",
          "icon": "clock-rotate-left"
        },
        {
          "anchor": "Status",
          "href": "https://quadrillion.io/status",
          "icon": "signal"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.png",
    "dark": "/logo/dark.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Website",
        "href": "https://quadrillion.io"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Download",
      "href": "https://quadrillion.io/dashboard/setup"
    }
  },
  "footer": {
    "socials": {
      "website": "https://quadrillion.io",
      "github": "https://github.com/quadrillion-labs"
    }
  }
}
